	editor = vim
	pager = less -r

[http]
	sslVerify = false

[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	l  = log --pretty=format:\"%h %ar\t(%an)\t: %s\" --date=short -20
	df = diff
	dfw = diff --color-words

